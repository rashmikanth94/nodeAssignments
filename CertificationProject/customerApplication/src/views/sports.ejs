<div class="ml-5 pb-5">
    <div class="cardsHolder">
        <%for(let i=0; i<data.length;i++){%>    
            <div class="m-2">
                <div class="card" style="width: 20rem;">
                    <img src=<%= data[i].urlToImage %> class="card-img-top" style="height: 150px;">
                    <div class="card-body" style="min-height: 290px;">
                    <div class="card-title bold"><%=data[i].title %></div>
                    <p class="card-text"><%=data[i].content %></p>
                    </div>
                </div>
            </div>
        
        <%}%>
    </div>
